<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="demoqa" verbose="1" thread-count="3" parallel="tests" preserve-order="true">

    <test name="chrome-general-menu">
        <parameter name="browser" value="CHROME"/>
